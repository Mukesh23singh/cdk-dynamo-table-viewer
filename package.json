{
  "name": "cdk-dynamo-table-viewer",
  "version": "3.0.2",
  "scripts": {
    "build": "jsii",
    "watch": "jsii -w",
    "package": "jsii-pacmak -vv",
    "test": "jest",
    "cdk": "cdk"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "com.github.eladb.dynamotableviewer",
        "maven": {
          "groupId": "com.github.eladb",
          "artifactId": "cdk-dynamo-table-viewer"
        }
      },
      "python": {
        "distName": "cdk-dynamo-table-viewer",
        "module": "cdk_dynamo_table_viewer"
      }
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ]
  },
  "author": {
    "email": "elad.benisrael@gmail.com",
    "name": "Elad Ben-Israel",
    "url": "https://github.com/eladb"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/eladb/cdk-dynamo-table-viewer.git"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "12.0.12",
    "jest": "^24.8.0",
    "jsii": "^0.14.0",
    "jsii-pacmak": "^0.14.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^0.38.0",
    "@aws-cdk/aws-dynamodb": "^0.38.0",
    "@aws-cdk/aws-lambda": "^0.38.0",
    "@aws-cdk/core": "^0.38.0"
  },
  "peerDependencies": {
    "@aws-cdk/aws-apigateway": "^0.38.0",
    "@aws-cdk/aws-dynamodb": "^0.38.0",
    "@aws-cdk/aws-lambda": "^0.38.0",
    "@aws-cdk/core": "^0.38.0"
  }
}
